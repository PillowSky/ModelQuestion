<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="ModelQuestion.App"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <System:String x:Key="SelectDirectory">Select Directory</System:String>
        <System:String x:Key="SavePath">Save Path</System:String>
        <System:String x:Key="StartTest">Start Test</System:String>
        <System:String x:Key="BadDirectoryText">No available test case</System:String>
        <System:String x:Key="BadDirectoryTitle">Bad directory</System:String>
        <System:String x:Key="ResultSavedText">Test result has been saved to </System:String>
        <System:String x:Key="ResultSavedTitle">Test result saved</System:String>
        <System:String x:Key="PleaseSelectText">Please select to continue</System:String>
        <System:String x:Key="PleaseSelectTitle">Please select to continue</System:String>
        <System:String x:Key="ConfirmSubmitText">Are you sure to submit?</System:String>
        <System:String x:Key="ConfirmSubmitTitle">Are you sure to submit?</System:String>

    </Application.Resources>
</Application>
